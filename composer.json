{
  "name": "marshmallow/nova-sortable",
  "description": "This Laravel Nova package allows you to reorder models in a Nova resource's index view using drag & drop.",
  "keywords": [
    "laravel",
    "nova",
    "eloquent-sortable",
    "marshmallow"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "laravel/nova": "^4.0",
    "marshmallow/nova-resource-click": "^1.0",
    "optimistdigital/nova-translations-loader": "^4.0.0",
    "spatie/eloquent-sortable": "^4.0.1"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Marshmallow\\NovaSortable\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Marshmallow\\NovaSortable\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
